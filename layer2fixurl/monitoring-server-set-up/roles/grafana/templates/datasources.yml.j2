apiVersion: 1

datasources:
  - name: Prometheus
    type: prometheus
    access: proxy
    uid: prometheus
    url: http://localhost:{{ prometheus_port }}
    isDefault: true
    editable: true

  - name: Loki
    type: loki
    access: proxy
    uid: loki
    url: http://localhost:{{ loki_port }}
    editable: true